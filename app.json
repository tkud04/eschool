{
  "expo": {
    "name": "E-School NG",
    "description": "Mobile app for E-School NG",
    "slug": "eschool-ng",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#072865"
    },
    "notification": {
      "icon": "./assets/images/notification.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "ios": {
	  "bundleIdentifier": "com.primetech.eschool",
      "supportsTablet": true
    },
	"android": {
      "package": "com.primetech.eschool",
	  "permissions": ["NOTIFICATIONS"]
    }
  }
}
